$('#spinner').hide(300);
<% if @account.errors.any? %>
  $("#flash_notice").hide(300);
  $("#form_errors").html("<%= escape_javascript( render(partial: 'shared/error_messages', locals: {target: @account})) %>");
<% else %>
  postFlashMsg('#flash_notice','success', 'Successfully saved account.');
  $("#inv_form").html("<%= escape_javascript( raw render(partial: 'shared/invoice_form') ) %>");
<% end %>
