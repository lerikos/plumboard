<% if @account.errors.any? %>
  $("#notice").hide(300);
  $("#form_errors").html("<%= escape_javascript( render(partial: 'shared/error_messages', locals: {target: @account})) %>");
<% else %>
  $("#notice").html("<%= escape_javascript(flash.now[:notice])%>");
  $("#notice").show(300);
  $("#inv_form").html("<%= escape_javascript( raw render(partial: @target)) %>");
<% end %>

$('#spinner').hide(300);


