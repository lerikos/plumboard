<% provide(:title, 'Manage Pixis') %>
<%= content_for :subhead do %>
  <%= render :partial => 'shared/navbar', locals: { page_title: 'Manage Pixis', atype: 'local', menu_name: 'Manage Pixis' } %>
<% end %>

<%= content_for :content do %>
  <div id="pendingOrder">
	<% unless @listings.blank? %>
	  <div id="top-header" class='sm-top nav pull-right'>
	    <%= link_to 'Export as CSV file', listings_path(format: "csv"), class: 'btn btn-small' %>
	  </div>
	<% end %>
    <%= render :partial => 'shared/index_list' %>
  </div>
  <div id="sm-top" class="pg-entry">
    <%= page_entries_info @listings, :model => 'pixis' %>
  </div>
<% end %>

