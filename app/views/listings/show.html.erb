<% if @listing %>
  <% provide(:title, @listing.nice_title(false)) %>
  <%= content_for :subhead do %>
    <%= render partial: 'shared/navbar', locals: { menu_name: 'Pixi' } %>
  <% end %>
  <%= content_for :content do %>
      <%= render partial: 'shared/show_listing', locals: {listing: @listing, edit_mode: false} %>
  <% end %>
<% end %>
