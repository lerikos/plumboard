<%= link_to 'Edit', edit_temp_listing_path(listing), 'data-role'=>'button', 'data-theme'=>'b', 'data-inline'=>'true' %>
<% if listing.edit? %>
  <%= link_to 'Cancel', root_path, 'data-role'=>'button', 'data-inline'=>'true', data: { confirm: msg } %>
<% else %>
  <%= link_to 'Remove', listing, method: :delete, confirm: msg, 'data-role'=>'button', 'data-theme'=>'b', 'data-inline'=>'true' %>
<% end %>

