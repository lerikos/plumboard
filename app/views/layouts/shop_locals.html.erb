<!DOCTYPE html>
<html>
<head>
  <%= render 'layouts/title' %>
</head>
<body>
  <div id='home-wrap'>
    <div class="sls-bkgd">
      <%= yield :topcontent %>
    </div>
    <div class="sls-container">
      <div id="spinner" class="round" style="display:none"></div>
      <%= render 'layouts/flash_messages', flash: flash %>
      <%= yield :content %>
    </div>
    <div class="footer-container">
      <%= render 'layouts/footer' %>
      <%= javascript_include_tag "application", "https://maps.googleapis.com/maps/api/js?key=#{API_KEYS['google']['api_key']}" %>
    </div>
  </div>
</body>
</html>
