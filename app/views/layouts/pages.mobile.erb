<!DOCTYPE html>
<html>
  <head> 
    <%= yield :title %> 
    <%= stylesheet_link_tag "mobile", :media => "all" %>  
    <%= render 'layouts/jqm' %>
    <%= csrf_meta_tag %>  	
  </head>
  <body>  
    <div id="app" data-role="page" data-iscroll="enable" data-cache="never" class='home-pg'> 
      <div data-role="header" data-theme="d" id="app-header">
      	<%= yield :head %>
      	<%= yield :srch_content %>
      </div>
    
      <div class="app-content" data-theme="d" data-role="content">   			
 	<div data-iscroll="scroller" class="listpg">
	<%= yield :subhead %>
	  <%= render 'mobile/notice_alert'%>
      	  <%= yield :content %>
        </div>
      </div>
    </div>  
  </body> 
</html>
