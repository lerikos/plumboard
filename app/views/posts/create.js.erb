<% if @post.errors.any? %>
  $("#form_errors").html("<%= escape_javascript( render(partial: 'shared/error_messages', locals: {target: @post})) %>");
<% else %>
  $("#post_form").html("<%= escape_javascript( raw render(partial: 'shared/contact', locals: {listing: @listing})) %>");
<% end %>

if($('.show-pixi-bar').length > 0) {
    uiLoading(false);
  }
else
  $('#spinner').hide(300);

