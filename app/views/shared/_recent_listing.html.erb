<li id="<%= listing.id %>">
  <%= render partial: 'shared/show_photo', locals: {model: listing, file_name: "myimage", psize: '80x80', display_cnt: 0 } %>
  <div class='span5'>
    <div class='recent'>
      <%= link_to listing.title, listing %>
    </div><br />
    <span class="gray-content width240"><%= listing.site_name %></span>
    <div class="pull-right" data-time="<%= listing.created_at.to_i %>"> 
      <span class="gray-content">
        Posted <%= ts_in_words(listing.created_at) %>.
      </span>
    </div>
  </div>
  <div class='clear-all'></div>
</li>
