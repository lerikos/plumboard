<%= link_to 'Prev', edit_temp_listing_path(listing), class: 'btn btn-large btn-primary stnd-btn' %>
<% unless listing.new_status? %>
  <%= link_to 'Cancel', listings_path, confirm: msg, class: 'btn btn-large btn-primary stnd-btn' %>
<% else %>
  <%= link_to 'Cancel', listing, method: :delete, confirm: msg, class: 'btn btn-large btn-primary stnd-btn' %>
<% end %>

