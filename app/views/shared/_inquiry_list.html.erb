<% unless @inquiries.blank? %>
  <table class="table table-hover table-condensed table-bordered">
    <thead>
    <tr>
      <th class='center-wrapper'>Inquiry #</th>
      <th class='center-wrapper'>Inquiry Date</th>
      <th class='center-wrapper'>User Name</th>
      <th class='center-wrapper'>Subject</th>
      <th class='center-wrapper'>Details</th>
      <th class='center-wrapper'>Status</th>
      <th></th>
    </tr>
    </thead>
    <tbody>
        <%= render partial: 'shared/inquiry_row', collection: @inquiries, as: :inquiry %>
    </tbody>
  </table>
<% else %>
  <div class='center-wrapper'>No inquiries found.</div>
<% end %>
<%= paginate_list 'inq-list', @inquiries %>
