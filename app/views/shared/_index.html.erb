<% provide(:title, page_title) %>
<%= content_for :subhead do %>
  <%= render partial: 'shared/navbar', locals: { menu_name: page_title } %>
<% end %>

<%= render 'shared/map' %>

<%= content_for :content do %>
  <div id="pxboard">
    <%= render 'shared/listings' %>
  </div> 

  <div id="px-nav" class='center-wrapper nxt-pg-spinner'>
    <%= render 'shared/listing_next_page' %>
  </div>

  <%= link_to 'Scroll', '#', class: 'scrollup' %>
<% end %>
