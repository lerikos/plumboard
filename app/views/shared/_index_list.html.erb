<table class="table table-hover table-condensed table-bordered">
  <thead>
  <tr>
    <th></th>
    <th>Title</th>
    <th>Description</th>
    <th>Posted By</th>
    <th>Posted At</th>
  </tr>
  </thead>
  <tbody>
  <%= render partial: 'shared/listing_row', collection: @listings, as: :listing %>
  </tbody>
</table>

<div class="nav pull-right">
  <%= will_paginate @listings, :param_name => 'pending_page' if @listings.respond_to?(:total_pages) %>
</div> 
